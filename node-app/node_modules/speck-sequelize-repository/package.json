{
  "name": "speck-sequelize-repository",
  "version": "0.1.0",
  "description": "Repositories using Speck",
  "main": "src/index.js",
  "scripts": {
    "lint": "standard -v \"src/**/*.js\"",
    "lint:fix": "standard --fix \"src/**/*.js\"",
    "lint:watch": "watch -p \"+(src)/**/*.js\" -c \"yarn lint\"",
    "test": "npm run lint && NODE_ENV=test NODE_PATH=./ mocha \"src/**/*.test.js\" --colors --require tests/support.js --reporter spec",
    "test:watch": "npm test -- --watch",
    "coverage": "npm run lint && nyc npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scup/speck-sequelize-repository"
  },
  "keywords": [
    "Javascript",
    "Entities",
    "Repositories"
  ],
  "author": "Scup",
  "license": "GNU",
  "bugs": {
    "url": "https://github.com/scup/speck-sequelize-repository/issues"
  },
  "dependencies": {
    "joi": "^9.1.1",
    "object-mapper": "^3.0.1",
    "pg": "^6.1.2",
    "sequelize": "^3.24.4",
    "sequelize-cli": "^2.4.0",
    "speck-entity": "^0.1.3",
    "uuid": "^3.0.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "faker": "^3.1.0",
    "lodash": "^4.17.4",
    "mocha": "^3.1.1",
    "moment": "^2.17.1",
    "nyc": "^8.3.1",
    "rosie": "^1.6.0",
    "sinon": "3.2.1",
    "standard": "^8.3.0",
    "watch-cli": "^0.2.1"
  },
  "nyc": {
    "branches": 95,
    "check-coverage": true,
    "functions": 95,
    "lines": 95,
    "statements": 95,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "node_modules/**/*"
    ]
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "it",
      "after",
      "afterEach",
      "before",
      "beforeEach"
    ]
  }
}
