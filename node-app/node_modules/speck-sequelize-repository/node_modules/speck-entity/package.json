{
  "name": "speck-entity",
  "version": "0.1.9",
  "description": "Domain entities with reactive validation",
  "main": "lib/index.js",
  "scripts": {
    "test": "find ./spec -name '*.spec.js' | NODE_ENV=test xargs mocha --compilers js:babel-register --reporter spec",
    "test:watch": "npm test -- --watch",
    "coverage": "nyc npm test",
    "build": "babel src --out-dir lib --presets es2015",
    "prepublish": "npm run build"
  },
  "nyc": {
    "branches": 95,
    "check-coverage": true,
    "functions": 95,
    "lines": 95,
    "statements": 95,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "spec/**/*",
      "node_modules/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scup/speck"
  },
  "keywords": [
    "Javascript",
    "Entities",
    "React",
    "Entities"
  ],
  "author": "Scup",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scup/speck/issues"
  },
  "homepage": "https://github.io/scup/speck",
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "lodash": "^4.6.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.0",
    "chai": "^3.5.0",
    "faker": "^3.0.1",
    "istanbul": "^0.4.5",
    "joi": "^9.0.4",
    "mocha": "^3.0.2",
    "nodemon": "^1.9.2",
    "nyc": "^8.1.0",
    "react": "^0.14.3",
    "sinon": "^1.17.5"
  }
}
